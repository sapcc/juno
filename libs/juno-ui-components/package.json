    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "\\.mjs$": "babel-jest",
      "^.+\\.svg$": "<rootDir>/test/__mocks__/svg.js"
    },
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/__mocks__/styleMock.js",
      "@material-design-icons/svg/.+\\.svg$": "<rootDir>/test/__mocks__/svgLib.js"
