    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-merge-refs)/)"
